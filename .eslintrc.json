{
	"plugins": [
		"prettier",
		"@typescript-eslint",
		"react",
		"jsx-a11y",
		"react-hooks"
	],
	"extends": [
		"prettier",
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:react/recommended",
		"plugin:jsx-a11y/recommended",
		"plugin:react-hooks/recommended"
	],
	"rules": {
		"no-console": "warn",
		"no-unused-vars": "warn",
		"linebreak-style": ["error", "unix"]
	}
}
